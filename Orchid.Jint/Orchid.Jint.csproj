    <Company>Enklu</Company>
    <Authors>Enklu</Authors>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Jint-Enklu" Version="2.9.1" />
  </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="../Orchid/Orchid.csproj" />
